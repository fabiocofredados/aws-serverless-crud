    "jest": "^29.7.0",
    "serverless": "^3.0.0",
    "serverless-dynamodb": "^0.2.54"
  },
  "devDependencies": {
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-offline": "^8.8.1"
  }
}
